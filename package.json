  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk=0.0.0.0 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:debug": "node --inspect=0.0.0.0 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/jest-e2e.json --runInBand",
    "test:overall": "jest --config jest-overall.json --runInBand --no-cache --forceExit",
    "artisan": "node dist/artisan.js",
    "migrate": "typeorm migration:run",
    "seed": "node dist/artisan.js seed",
    "preinit": "nest build && vue-cli-service build --no-clean",
    "init": "typeorm migration:run",
    "postinit": "node dist/artisan.js seed && node dist/artisan.js syncToggl",
    "vue:serve": "vue-cli-service serve --port 3001",
    "vue:build": "vue-cli-service build --no-clean"
  },
